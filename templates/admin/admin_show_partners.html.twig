{% extends 'base.html.twig' %}

{% block title %}Partenaires{% endblock %}

{% block body %}


<div class="example-wrapper">
    <h1>Liste des partenaires ({{ partners|length }})</h1>

    {{ form(searchForm) }}
    <div>
        {% for partner in partners %}
            <h3>{{ partner.name }}</h3>
            <p>{{ partner.user.email }}</p>
            <a href="{{ path('app_admin_show_partner', {slug: partner.slug }) }}">Voir la fiche du partenaire</a>
        {% endfor %}
    </div>
</div>
{% endblock %}
